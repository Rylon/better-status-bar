{
	"name": "better-status-bar",
	"displayName": "Better Status Bar",
	"description": "Makes the status bar better.",
	"version": "0.0.1",
	"publisher": "rylon",
	"repository": {
		"type": "git",
		"url": "https://github.com/rylon/better-status-bar"
	},
	"bugs": {
		"url": "https://github.com/rylon/better-status-bar/issues"
	},
	"license": "SEE LICENSE IN COPYING",
	"engines": {
		"vscode": "^1.49.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:better-status-bar.helloWorld",
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "better-status-bar.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.49.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"eslint": "^7.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	},
	"dependencies": {
		"package.json": "^0.0.0"
	}
}
